{
  "name": "@skillhub/cli",
  "version": "1.0.0",
  "description": "CLI tool for SkillHub enterprise skill marketplace",
  "type": "module",
  "bin": {
    "skillhub": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --sourcemap",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "skillhub",
    "cli",
    "skills",
    "ai",
    "agents",
    "claude",
    "copilot",
    "cursor"
  ],
  "author": "SkillHub",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "ora": "^8.1.0",
    "semver": "^7.7.4",
    "undici": "^7.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@types/semver": "^7.7.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skillhub/cli.git"
  }
}
